{
    "name": "tomatophp/filament-menus",
    "type": "library",
    "description": "Menu Database builder to use it as a navigation on Filament Panel or as a Livewire Component",
    "keywords": [
        "laravel",
        "menus",
        "generator",
        "url",
        "routes",
        "views",
        "filamentphp",
        "plugin"
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "TomatoPHP\\FilamentMenus\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "TomatoPHP\\FilamentMenus\\FilamentMenusServiceProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "Fady Mondy",
            "email": "info@3x1.io"
        }
    ],
    "require": {
        "php": "^8.1|^8.2",
        "tomatophp/console-helpers": "^1.1",
        "tomatophp/filament-icons": "^1.0",
        "filament/filament": "^3.0.0",
        "filament/notifications": "^3.0.0",
        "filament/spatie-laravel-translatable-plugin": "^3.0.0"
    }
}
